{% extends 'base.html.twig' %}

{% block title %}Medecin{% endblock %}

{% block body %}
    <h1>Medecin</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medecin.id }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ medecin.firstName }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ medecin.lastName }}</td>
            </tr>
            <tr>
                <th>Experience</th>
                <td>{{ medecin.experience }}</td>
            </tr>
            <tr>
                <th>Disponible</th>
                <td>{{ medecin.disponible ? medecin.disponible|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Tarif</th>
                <td>{{ medecin.tarif }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ medecin.tel }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_medecin_index') }}">back to list</a>

    <a href="{{ path('app_medecin_edit', {'id': medecin.id}) }}">edit</a>

    {{ include('medecin/_delete_form.html.twig') }}
{% endblock %}
